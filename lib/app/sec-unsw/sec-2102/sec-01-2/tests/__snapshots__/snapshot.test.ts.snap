exports[`app(unsw::2102::01.2).widgets > strengthsAndLimitations 1`] = `"<div class="container dashbox" style="padding: 16px; border: var(--border-white-dash);"><h2>Strengths And Limitations</h2><div class="c2 pre--c2"><div class="container pre--container"><h4>Strengths</h4><ul class="no-item-padding" style="margin-block: 0px;"><li>Provides a framework for the growth of capital.</li><li>Provides a means to isolate TFP via subtraction of labour and capital.</li></ul></div><div class="container pre--container"><h4>Limitations</h4><ul class="no-item-padding" style="margin-block: 0px;"><li>No answer to rate at which a country invests.</li><li>No answer to how the steady moves.</li><li>No answer to long term growth.</li></ul></div></div></div>"`;

exports[`app(unsw::2102::01.2).widgets > transitionDynamics 1`] = `
"<div class="container dashbox" style="padding: 16px; border: var(--border-white-dash);"><h2>Principle of Transition Dynamics</h2><p>
    You can observe this playing around with the model above
    and seeing how rate of change is faster the further away
    the current accumulation of capitial is from it's steady
    state. If the steady state spikes the growth rate is large
    number that slowly decreases. If there's a large drop in
